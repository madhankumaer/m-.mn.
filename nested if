#include<stdio.h>
void main()
{
  int a,b,c;
  scanf("%d %d %d",&a,&b,&c);
   
   if(a>b&&a>c)
   {
   printf("a is the great ");
   }
   else
   {
       if(b>c&&b>a)
       {
     printf("b is the great "); 
           
       }
   
       else
       {
           printf("c is great ");
           
       }
       
   }
} 
